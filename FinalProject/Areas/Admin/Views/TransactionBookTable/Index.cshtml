@model IEnumerable<FinalProject.Models.TransactionBookTable>

@{
    ViewData["Title"] = "Index";
}


<section class="section col-lg-12">

    <div class="row">
        <div class="col-lg-12">

            <div class="card">
                <div class="card-body">
<table class="table table-striped">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TransactionBookTableFullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TransactionBookTableEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TransactionBookTableMobileNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TransactionBookTableDate)
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionBookTableFullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionBookTableEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionBookTableMobileNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionBookTableDate)
            </td>
            
            
        </tr>
}
    </tbody>
</table>

</div>
</div>
</div>
</div>
</section>