@model FinalProject.Models.MasterContactUsInformation

@{
    ViewData["Title"] = "Edit";
}


<div class="row justify-content-center">
<div class="col-lg-7">
<div class="card">
    <div class="card-body">
        <form asp-action="Edit" class="row g-3" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="MasterContactUsInformationId" />
            <div class="form-group">
                <label asp-for="MasterContactUsInformationIdesc" class="control-label"></label>
                <input asp-for="MasterContactUsInformationIdesc" class="form-control" />
                <span asp-validation-for="MasterContactUsInformationIdesc" class="text-danger"></span>
            </div>
            <div class="form-group">
                 <label asp-for="MasterContactUsInformationImageUrl" class="control-label"></label>
                <input asp-for="MasterContactUsInformationImageUrl"  class="form-control" />
                <span asp-validation-for="MasterContactUsInformationImageUrl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MasterContactUsInformationRedirect" class="control-label"></label>
                <input asp-for="MasterContactUsInformationRedirect" class="form-control" />
                <span asp-validation-for="MasterContactUsInformationRedirect" class="text-danger"></span>
            </div>
            <div hidden class="form-group form-check">
                <label  class="form-check-label">
                    <input hidden class="form-check-input" asp-for="IsActive" />  @Html.DisplayNameFor(model => model.IsActive)
                </label>
            </div>
            <div hidden class="form-group form-check">
                <label  class="form-check-label">
                    <input hidden class="form-check-input" asp-for="IsDelete" /> @Html.DisplayNameFor(model => model.IsDelete)
                </label>
            </div>
            <div hidden class="form-group">
                <input asp-for="CreateUser"  class="form-control" />
                <span asp-validation-for="CreateUser" class="text-danger"></span>
            </div>
            <div hidden class="form-group">
                <input asp-for="CreateDate"  class="form-control" />
                <span asp-validation-for="CreateDate" class="text-danger"></span>
            </div>

           


                <div class="form-group">
                    <input type="submit" value="Edit" class="btn btn-primary" />
                </div>
        </form>
    </div>
</div>
</div>
<div class="text-center">
    <a asp-action="Index">Back to List</a>
</div>

</div>