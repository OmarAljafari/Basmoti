@model FinalProject.Models.MasterCategoryMenu

@{
    ViewData["Title"] = "Edit";
}


<div class="row justify-content-center">
    <div class="col-lg-7">
        <div class="card">
            <div class="card-body">
        <form asp-action="Edit" class="row g-3">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="MasterCategoryMenuId" />
            <input asp-for="IsActive" hidden class="form-control" />
            <div class="form-group">
                <label asp-for="MasterCategoryMenuName" class="control-label"></label>
                <input asp-for="MasterCategoryMenuName" class="form-control" />
                <span asp-validation-for="MasterCategoryMenuName" class="text-danger"></span>
            </div>
           
            <div hidden class="form-group">    
                <input asp-for="CreateDate"  class="form-control" />
                <span asp-validation-for="CreateDate" class="text-danger"></span>
            </div>
            <div hidden class="form-group">
                <input asp-for="CreateUser"  class="form-control" />
                <span asp-validation-for="CreateUser" class="text-danger"></span>
            </div>
            <div class="form-group mt-3">
                <input type="submit" value="Edit" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
</div>

<div class="text-center">
    <a asp-action="Index">Back to List</a>
</div>

</div>